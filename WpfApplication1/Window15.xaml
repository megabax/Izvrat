<Window x:Class="WpfApplication1.Window15"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="Window15" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Viewbox Grid.Row="0">
            <Canvas Width="250" Height="100">
                <Ellipse Canvas.Left="10" Canvas.Top="20" Width="50" Height="30" Fill="Red"></Ellipse>
                <Rectangle RadiusX="30" RadiusY="10" Canvas.Left="50" Canvas.Top="11" Fill="Blue" Width="150" Height="81"></Rectangle>
            </Canvas>
        </Viewbox>
        <Viewbox Grid.Row="1">
            <Ellipse Canvas.Left="10" Canvas.Top="20" Width="50" Height="30" Fill="Red"></Ellipse>
        </Viewbox>
    </Grid>

</Window>
